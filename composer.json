{
  "name": "cococollective/coco-ux",
  "description": "Drupal module: Provides UX improvements.",
  "type": "drupal-module",
  "license": "GPL-2.0+",
  "require": {
    "drupal/drupal": "7.*",
    "drupal/features": "7.*",
    "drupal/strongarm": "7.*",
    "wikimedia/composer-merge-plugin": "^1.3"
  },
  "minimum-stability": "dev",
  "prefer-stable": "true",
  "extra": {
    "branch-alias": {
      "dev-7.x-0.x": "7.0.x-dev"
    },
    "merge-plugin": {
      "repositories": [
        {
          "type": "composer",
          "url": "https://packagist.drupal-composer.org"
        }
      ]
    }
  },
  "authors": [
    {
      "name": "Sierk Beij",
      "email": "sierk@cococollective.org"
    }
  ]
}
